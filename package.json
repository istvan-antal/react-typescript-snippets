{
	"name": "react-typescript-snippets",
	"displayName": "React Typescript Snippets",
	"description": "Snippets that help developing React apps in typescript",
	"version": "0.0.1",
	"publisher": "istvan-antal",
	"engines": {
		"vscode": "^1.8.0"
	},
	"categories": [
		"Snippets"
	],
	"contributes": {
		"snippets": [
			{
				"language": "typescriptreact",
				"path": "./snippets.json"
			}
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/istvan-antal/react-typescript-snippets"
	},
	"scripts": {
		"publish": "vsce publish --packagePath build/react-typescript-snippets.vsix",
		"build": "vsce package -o build/react-typescript-snippets.vsix",
		"prebuild": "mkdirp build"
	},
	"devDependencies": {
		"mkdirp": "^0.5.1",
		"vsce": "^1.63.0"
	}
}
